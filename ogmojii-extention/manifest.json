{
  "manifest_version": 2,
  "name": "OGMojii",
  "version": "1.0",
  "description": "Use emoticons on Facebook",
  "permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.facebook.com/*"
      ],
      "js": [
        "ogmojii.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}